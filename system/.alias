function composer7 {
     \docker run \
        --rm -it \
        -e PHP_UID=$(id -u) \
        -e PHP_GID=$(id -g) \
        -e PHP_SHELL=1 \
        -v $PWD:/opt/www \
        -v ~/.composer:/home/php/composer \
        -v ~/.ssh:/home/php/.ssh:ro \
        -w /opt/www \
        seeruk/php-fpm:7.1-dev \
        /home/php/composer/composer.phar $@
}

function composer5.6 {
     \docker run \
        --rm -it \
        -e PHP_UID=$(id -u) \
        -e PHP_GID=$(id -g) \
        -e PHP_SHELL=1 \
        -v $PWD:/opt/www \
        -v ~/.composer:/home/php/composer \
        -v ~/.ssh:/home/php/.ssh:ro \
        -w /opt/www \
        seeruk/php-fpm:5.6-dev \
        /home/php/composer/composer.phar $@
}

